
CREATE TABLE `Stock db`
(
 `Uuid`            tinytext NOT NULL ,
 `Weight`          numeric unsigned zerofill NOT NULL ,
 `Units`           numeric unsigned zerofill NOT NULL ,
 `Size`            numeric unsigned zerofill NOT NULL ,
 `Date of arrival` datetime NOT NULL ,
 `Position`        mediumtext NOT NULL ,
 `Cost`            numeric unsigned zerofill NOT NULL ,
 `Profit`          numeric unsigned zerofill NOT NULL 
);

CREATE TABLE `Job db`
(
 `Uuid`                 tinytext NOT NULL ,
 `Current position`     tinytext NOT NULL ,
 `Destination position` tinytext NOT NULL ,
 `Priority`             numeric unsigned zerofill NOT NULL 
);

